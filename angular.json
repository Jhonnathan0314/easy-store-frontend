{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "sophie-store-frontend": {
      "projectType": "application",
      "schematics": {},
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/sophie-store-frontend",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              {
                "input": "src/assets/themes/arya-blue.css",
                "bundleName": "arya-blue",
                "inject": false
              },
              {
                "input": "src/assets/themes/arya-green.css",
                "bundleName": "arya-green",
                "inject": false
              },
              {
                "input": "src/assets/themes/arya-orange.css",
                "bundleName": "arya-orange",
                "inject": false
              },
              {
                "input": "src/assets/themes/arya-purple.css",
                "bundleName": "arya-purple",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-blue.css",
                "bundleName": "saga-blue",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-green.css",
                "bundleName": "saga-green",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-orange.css",
                "bundleName": "saga-orange",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-purple.css",
                "bundleName": "saga-purple",
                "inject": false
              }
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "sophie-store-frontend:build:production"
            },
            "development": {
              "browserTarget": "sophie-store-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "sophie-store-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "tsconfig.spec.json",
            "assets": [
              "src/favicon.ico",
              "src/assets"
            ],
            "styles": [
              "src/styles.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css",
              {
                "input": "src/assets/themes/arya-blue.css",
                "bundleName": "arya-blue",
                "inject": false
              },
              {
                "input": "src/assets/themes/arya-green.css",
                "bundleName": "arya-green",
                "inject": false
              },
              {
                "input": "src/assets/themes/arya-orange.css",
                "bundleName": "arya-orange",
                "inject": false
              },
              {
                "input": "src/assets/themes/arya-purple.css",
                "bundleName": "arya-purple",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-blue.css",
                "bundleName": "saga-blue",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-green.css",
                "bundleName": "saga-green",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-orange.css",
                "bundleName": "saga-orange",
                "inject": false
              },
              {
                "input": "src/assets/themes/saga-purple.css",
                "bundleName": "saga-purple",
                "inject": false
              }
            ],
            "scripts": []
          }
        }
      }
    }
  }
}
